[securitymessage]
name=Security alert message
Comment=Received a security alert message
default_presentation=21
default_sound=KDE_Error.wav
default_logfile=atmosphere.txt

[invaliddevice]
name=Invalid device opened
Comment=Tried to access a device without any kind of support
default_presentation=2
