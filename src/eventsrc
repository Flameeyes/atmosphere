Comment=ATMOSphere

[infomessage]
Name=Information message
Comment=Received an information message
default_presentation=0

[lowwarningmessage]
Name=Low warning message
Comment=Received a low warning message
default_presentation=0

[warningmessage]
Name=Warning message
Comment=Received a warning message
default_presentation=1
default_sound=KDE_Beep_Ahem.wav

[alertmessage]
Name=Alert message
Comment=Received an alert message
default_presentation=5
default_sound=KDE_Error.wav
